@{
}
@model PersonModel
<div class="container">
    <table>
        <thead>
            <tr>
                <th>EmpID</th>
                <th>Name</th>
                <th>Email</th>
                <th>Contact</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>@Model.PersonId</td>
                <td>@Model.PersonName</td>
                <td>@Model.PersonEmail</td>
                <td>@Model.PersonContact</td>
            </tr>
        </tbody>
    </table>
</div>